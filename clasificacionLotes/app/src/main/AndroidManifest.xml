<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.msig.clasificacionlotes">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ConfigActivity"
            android:label="Configuración de Conexión"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ListadoFormularios"
            android:label="Listado de Formularios Disponibles"
            android:parentActivityName=".MainActivity" />
        <activity android:name="com.msig.Formularios.ClasificacionLotes"
            android:label="Registro nuevo"
            android:parentActivityName=".MainActivity"/>
    </application>

</manifest>